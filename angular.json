{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "aud": {
      "projectType": "library",
      "root": "src/aud",
      "sourceRoot": "src/aud",
      "prefix": "aud",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "src/aud/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "src/aud/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "src/aud/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/aud/src/test.ts",
            "tsConfig": "src/aud/tsconfig.spec.json",
            "karmaConfig": "src/aud/karma.conf.js"
          }
        }
      }
    },
    "aud-examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "src/components-examples/aud-examples",
      "sourceRoot": "src/components-examples/aud-examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aud-examples",
            "index": "src/components-examples/aud-examples/src/index.html",
            "main": "src/components-examples/aud-examples/src/main.ts",
            "polyfills": "src/components-examples/aud-examples/src/polyfills.ts",
            "tsConfig": "src/components-examples/aud-examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/components-examples/aud-examples/src/favicon.ico",
              "src/components-examples/aud-examples/src/assets"
            ],
            "styles": ["src/components-examples/aud-examples/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/components-examples/aud-examples/src/environments/environment.ts",
                  "with": "src/components-examples/aud-examples/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aud-examples:build:production"
            },
            "development": {
              "browserTarget": "aud-examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aud-examples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/components-examples/aud-examples/src/test.ts",
            "polyfills": "src/components-examples/aud-examples/src/polyfills.ts",
            "tsConfig": "src/components-examples/aud-examples/tsconfig.spec.json",
            "karmaConfig": "src/components-examples/aud-examples/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/components-examples/aud-examples/src/favicon.ico",
              "src/components-examples/aud-examples/src/assets"
            ],
            "styles": ["src/components-examples/aud-examples/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  }
}
